{
  "name": "rizkussef/laravel-snake-to-camel-case",
  "description": "A Laravel middleware package to automatically convert API response keys from snake_case to camelCase for frontend compatibility, and optionally convert request keys from camelCase to snake_case",
  "keywords": [
    "laravel",
    "middleware",
    "snake_case",
    "camelCase",
    "request",
    "response",
    "api",
    "transform",
    "json",
    "key conversion",
    "laravel middleware",
    "case conversion",
    "camel snake"
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rizkussef\\LaravelSnakeToCamelCase\\": "src/"
    }
  },
  "authors": [
    {
      "name": "RizkUssef",
      "email": "rizkusgt67@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Rizkussef\\LaravelSnakeToCamelCase\\Providers\\SnakeToCamelServiceProvider"
      ]
    }
  }
}
